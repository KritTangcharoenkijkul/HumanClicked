; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Human Clicked"
run/main_scene="res://Scene/Cookienode2d.tscn"
config/features=PackedStringArray("4.1", "Mobile")
boot_splash/image="res://Assets/Font/Untitled-5.png"
config/icon="res://Assets/Font/Untitled-5.png"
config/macos_native_icon="res://Assets/Font/Untitled-5.png"
config/windows_native_icon="res://Assets/Font/Untitled-5.png"

[autoload]

Global="*res://Scene/Global.gd"

[display]

window/size/viewport_width=1440
window/size/viewport_height=2560
window/size/window_width_override=720
window/size/window_height_override=1280
window/stretch/mode="canvas_items"
window/handheld/orientation=1

[input_devices]

pointing/emulate_touch_from_mouse=true

[rendering]

renderer/rendering_method="gl_compatibility"
